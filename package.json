{
  "name": "codetalk-pure",
  "version": "1.0.0",
  "main": "src/main.ts",
  "type": "module",
  "scripts": {
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "format:ci": "node --run format:check",
    "start": "node src/main.ts",
    "start:dev": "node --watch src/main.ts",
    "lint:check": "eslint --max-warnings 0 \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint --fix \"{src,test}/**/*.ts\"",
    "lint:ci": "node --run lint:check",
    "typecheck": "tsc",
    "test:unitary": "vitest --config ./test/config/vitest.unitary.ts",
    "test:integration": "vitest --config ./test/config/vitest.integration.ts",
    "test:e2e:db": "vitest --config ./test/config/vitest.e2e-db.ts",
    "test:memory": "vitest --config ./test/config/vitest.memory.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@hono/node-server": "^1.12.0",
    "@hono/swagger-ui": "^0.4.0",
    "hono": "^4.7.2",
    "hono-openapi": "^0.3.1",
    "inversify": "^6.0.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.13.1",
    "pino": "^9.6.0",
    "zod": "^3.24.2",
    "zod-openapi": "^4.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.5",
    "eslint": "^9.21.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vitest": "^3.0.7"
  }
}
