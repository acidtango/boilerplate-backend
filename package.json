{
  "name": "bnext-challenge",
  "version": "1.0.0",
  "description": "Bnext challenge",
  "author": "Acid Tango",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"**/*.json\"",
    "start": "rimraf dist && nest start",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "typecheck": "tsc",
    "lint:check": "eslint --max-warnings 0 \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint --fix \"{src,apps,libs,test}/**/*.ts\"",
    "lint:ci": "yarn lint:check",
    "test": "jest -c test/config/unitary.json",
    "test:integration": "jest -c test/config/integration.json",
    "test:unitary:db": "jest -c test/config/unitary-db.json",
    "test:all:db": "jest -c test/config/all-db.json",
    "test:coverage": "yarn test:all:db --coverage",
    "test:ci": "yarn test:all:db",
    "migration:generate": "mikro-orm migration:create",
    "migration:run": "mikro-orm migration:up",
    "migration:revert": "mikro-orm migration:down",
    "run-checks": "yarn typecheck && yarn format && yarn lint"
  },
  "engines": {
    "node": "^16.13.0",
    "yarn": "~1.22.5"
  },
  "dependencies": {
    "@mikro-orm/cli": "4.5.9",
    "@mikro-orm/core": "4.5.9",
    "@mikro-orm/nestjs": "4.3.1",
    "@mikro-orm/postgresql": "4.5.9",
    "@mikro-orm/reflection": "4.5.9",
    "@nestjs/axios": "0.0.3",
    "@nestjs/common": "8.2.0",
    "@nestjs/core": "8.2.0",
    "@nestjs/platform-express": "8.2.0",
    "@nestjs/swagger": "5.1.4",
    "@nestjs/terminus": "8.0.1",
    "@nestjs/typeorm": "8.0.2",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "express": "4.17.1",
    "leaked-handles": "5.2.0",
    "morgan": "1.10.0",
    "nestjs-pino": "2.3.0",
    "node-fetch": "3.1.0",
    "pino-http": "5.8.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "swagger-ui-express": "4.1.6",
    "typeorm": "0.2.40",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@mikro-orm/migrations": "4.5.9",
    "@nestjs/cli": "8.1.4",
    "@nestjs/schematics": "8.0.4",
    "@nestjs/testing": "8.2.0",
    "@shopify/eslint-plugin": "40.4.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/morgan": "1.9.3",
    "@types/node": "16.11.8",
    "@types/node-fetch": "3.0.3",
    "@types/pino-http": "5.8.1",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "15.0.0",
    "eslint-config-import": "0.13.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.3.1",
    "prettier": "2.4.1",
    "supertest": "6.1.6",
    "tepper": "0.3.4",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.4"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/database/orm.config.ts",
      "./dist/database/orm.config.ts"
    ]
  }
}
